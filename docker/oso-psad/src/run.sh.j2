#!/bin/bash -e
{{ generated_header }}

echo -n "Running oso-{{ base_os }}-psad... "
{% if base_os == 'rhel7' %}
sudo docker run -ti --net=host --rm=true --name oso-psad oso-rhel7-psad $@
{% elif base_os == 'centos7' %}
sudo docker run -ti --net=host --rm=true --name oso-psad oso-centos7-psad $@
{% endif %}
echo "Done."
